#!/usr/bin/clitoris  ## -*- shell-script -*-

$ yuck gendsl "${srcdir}/subcmd_03.yuck"
changequote([,])dnl
divert([-1])

yuck_set_umbrella([plain], [])
yuck_set_umbrella_desc([plain], [Run plain tool.])
yuck_add_option([h], [help], [auto], [])
yuck_set_option_desc([h], [help], [], [display this help and exit])
yuck_add_option([V], [version], [auto], [])
yuck_set_option_desc([V], [version], [], [output version information and exit])

yuck_add_command([foo], [])
yuck_set_command_desc([foo], [Run plain tool in foo mode.])

yuck_add_command([bar], [])
yuck_set_command_desc([bar], [Run plain tool in bar mode.])

changecom([//])
divert[]dnl
$

## subcmd_03.clit ends here
