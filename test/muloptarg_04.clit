#!/usr/bin/clitoris  ## -*- shell-script -*-

$ yuck gendsl "${srcdir}/muloptarg_04.yuck"
changequote([,])dnl
divert([-1])

yuck_set_umbrella([plain], [])
yuck_add_option([h], [help], [auto], [])
yuck_set_option_desc([h], [help], [], [display this help and exit])
yuck_add_option([V], [version], [auto], [])
yuck_set_option_desc([V], [version], [], [output version information and exit])
yuck_add_option([x], [execute], [arg, LEVEL, mul, opt], []);
yuck_set_option_desc([x], [execute], [], [Execute thoroughly.])

changecom([//])
divert[]dnl
$

## muloptarg_04.clit ends here
